{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: milo",
			"command": "/usr/bin/aarch64-linux-gnu-gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
				
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/aarch64-linux-gnu-gcc"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: aarch64-linux-gnu-gcc build active file",
			"command": "/usr/bin/aarch64-linux-gnu-gcc",
			"args": [
				"-o",
				"${workspaceFolder}/Examen",
				"${workspaceFolder}/Examen.c",
				"-lpaho-mqtt3c",
				"-I/usr/local/include",
				"-L/usr/local/lib",
				"-1pthread",
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/aarch64-linux-gnu-gcc"
		}	
	]
}